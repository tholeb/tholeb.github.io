{
	"name": "tholeb.github.io",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"lint": "eslint --ext .ts,.tsx ./src",
		"lint:fix": "npm run lint -- --fix",
		"predeploy": "npm run lint:fix && npm run build && cp CNAME build/CNAME",
		"deploy": "NODE_ENV=production gh-pages -d build -m \"Deployed latest build : $(git show -s --format='%h' | cat) \n > git show -s --format='%h: %s'\""
	},
	"dependencies": {
		"@emotion/react": "^11.10.4",
		"@emotion/styled": "^11.10.4",
		"@fontsource/roboto": "^4.5.8",
		"@mui/icons-material": "^5.10.6",
		"@mui/material": "^5.10.8",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-tsparticles": "^2.3.3",
		"sass": "^1.55.0",
		"tailwind": "^4.0.0",
		"tsparticles": "^2.3.4"
	},
	"devDependencies": {
		"@swc/core": "^1.3.6",
		"@types/react": "^18.0.17",
		"@types/react-dom": "^18.0.6",
		"@typescript-eslint/eslint-plugin": "^5.40.0",
		"@typescript-eslint/parser": "^5.40.0",
		"@vitejs/plugin-react": "^2.1.0",
		"autoprefixer": "^10.4.12",
		"eslint": "^8.25.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-react": "^1.1.7",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-simple-import-sort": "^8.0.0",
		"postcss": "^8.4.18",
		"prettier": "^2.7.1",
		"tailwindcss": "^3.1.8",
		"ts-node": "^10.9.1",
		"typescript": "^4.6.4",
		"vite": "^3.1.0"
	}
}
