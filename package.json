		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-tsparticles": "^2.3.3",
		"sass": "^1.55.0",
		"tailwind": "^4.0.0",
		"tsparticles": "^2.3.4",
		"tsparticles-engine": "^2.3.3"
	},
	"devDependencies": {
		"@babel/core": "^7.19.3",
		"@swc/core": "^1.3.8",
		"@types/node": "^18.11.0",
		"@types/react": "^18.0.21",
		"@types/react-dom": "^18.0.6",
