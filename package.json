		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-simple-import-sort": "^8.0.0",
		"postcss": "^8.4.18",
		"prettier": "^2.7.1",
		"tailwindcss": "^3.1.8",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.4",
		"vite": "^3.1.8",
